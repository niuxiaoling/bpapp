<template name="417ba093">
  <view class="_view data-v-f64ef518">
    <view class="_view data-v-f64ef518 detailsText">
      <view class="_view data-v-f64ef518 detailsTitle">{{tasksDetails.taskTitle}}</view>
      <view class="_view data-v-f64ef518 detailsTips">请将下面得文本图片复制发送朋友圈（点击图片保存到手机）</view>
      <view class="_view data-v-f64ef518 detailsContent">
        <text class="_text data-v-f64ef518"> {{tasksDetails.copyWriting}} </text>
      </view>
      <view class="_view data-v-f64ef518">
        <button class="_button data-v-f64ef518 pastbut" bindtap="handleProxy" data-eventid="{{'vxB-0'}}" data-comkey="{{$k}}">复制文案</button>
      </view>
    </view>
    <view class="_view data-v-f64ef518 detailsImg">
      <view class="_view data-v-f64ef518 ImgTitle">图片信息</view>
      <view class="_view data-v-f64ef518 imgContent" wx:key="img" key="{{img}}" wx:for="{{copyImages}}" wx:for-index="index" wx:for-item="img">
        <image src="{{'https://www.sixkeep.com:8080/pics/' + img}}" mode="widthFix" bindtap="handleProxy" data-eventid="{{'ITj-1-'+index}}" data-comkey="{{$k}}" class="_image data-v-f64ef518"></image>
      </view>
    </view>
    <view class="_view data-v-f64ef518">
      <button class="_button data-v-f64ef518 getTasks" bindtap="handleProxy" data-eventid="{{'wly-2'}}" data-comkey="{{$k}}">立即领取</button>
    </view>
  </view>
</template>