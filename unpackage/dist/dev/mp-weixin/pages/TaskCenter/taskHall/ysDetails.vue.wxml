<template name="103ab253">
  <view class="_view data-v-063d3948">
    <view class="_view data-v-063d3948 detailsText">
      <view class="_view data-v-063d3948 detailsTitle">{{tasksDetails.taskTitle}}</view>
      <view class="_view data-v-063d3948 detailsTips">请将下面得文本图片复制发送朋友圈（点击图片保存到手机）</view>
      <view class="_view data-v-063d3948 detailsContent">
        <text class="_text data-v-063d3948"> {{tasksDetails.copyWriting}} </text>
      </view>
      <view class="_view data-v-063d3948">
        <button class="_button data-v-063d3948 pastbut" bindtap="handleProxy" data-eventid="{{'MaV-0'}}" data-comkey="{{$k}}">复制文案</button>
      </view>
    </view>
    <view class="_view data-v-063d3948 detailsImg">
      <view class="_view data-v-063d3948 ImgTitle">图片信息</view>
      <view class="_view data-v-063d3948 imgContent" wx:key="img" key="{{img}}" wx:for="{{copyImages}}" wx:for-index="index" wx:for-item="img">
        <image src="{{'https://www.sixkeep.com:8080/pics/' + img}}" mode="widthFix" bindtap="handleProxy" data-eventid="{{'Ogv-1-'+index}}" data-comkey="{{$k}}" class="_image data-v-063d3948"></image>
      </view>
    </view>
    <view class="_view data-v-063d3948">
      <button class="_button data-v-063d3948 getTasks" bindtap="handleProxy" data-eventid="{{'NSD-2'}}" data-comkey="{{$k}}">立即领取</button>
    </view>
  </view>
</template>