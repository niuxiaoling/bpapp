
page {
  background: #FFFFFF;
}
.detailsText,
.detailsImg {
  width: 96%;
  height: auto;
  margin: 2% auto;
  font-size: 30rpx;
  border: 1px solid #DADADA;
  box-shadow: 5rpx 5rpx 5rpx #999999;
}
.detailsTitle,
.ImgTitle {
  width: 96%;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 35rpx;
  color: #000;
  margin: 0 auto;
  border-bottom: 1px solid #DADADA;
}
.detailsTips {
  width: 96%;
  line-height: 50rpx;
  margin: 10rpx auto;
  color: #333333;
  border-bottom: 1px solid #DADADA;
}
.detailsContent {
  width: 96%;
  line-height: 50rpx;
  margin: 15rpx auto;
  color: #333333;
  text-indent: 20rpx;
}
.pastbut {
  width: 30%;
  height: 80rpx;
  border: 1px solid #007AFF;
  border-radius: 5rpx;
  color: #007AFF;
  margin: 20rpx auto;
}
.imgContent {
  width: 96%;
  margin: 10rpx auto;
}
.imgContent image {
  width: 96%;
  margin: 0 auto;
  border: 1px solid #DADADA;
  box-shadow: 5rpx 5rpx 5rpx #999999;
}
.getTasks {
  background: #E61532;
  width: 80%;
  color: #FFFFFF;
  margin: 30rpx auto;
}