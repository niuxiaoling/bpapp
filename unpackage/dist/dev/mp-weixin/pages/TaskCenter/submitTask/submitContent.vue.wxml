<template name="447394a6">
  <view class="_view data-v-824eecda">
    <view class="_view data-v-824eecda item-list">
      <text class="_text data-v-824eecda">1.提交的任务:</text>
      <text class="_text data-v-824eecda">{{tasksDetails.taskTitle}}</text>
    </view>
    <view class="_view data-v-824eecda item-list">
      <text class="_text data-v-824eecda">2.截取朋友圈图片并上传:</text>
    </view>
    <view class="_view data-v-824eecda item-list">
      <view class="_view data-v-824eecda btn-upload" bindtap="handleProxy" data-eventid="{{'gIn-0'}}" data-comkey="{{$k}}">请选择图片</view>
    </view>
    <view class="_view data-v-824eecda imgbox">
      <image src="{{uploadimg}}" mode="aspectFit" class="_image data-v-824eecda imgbox-img" bindtap="handleProxy" bindlongpress="handleProxy" data-eventid="{{'XFg-1'}}" data-comkey="{{$k}}"></image>
    </view>
    <view wx:if="{{uploadimg !=''}}" class="_view data-v-824eecda">
      <button class="_button data-v-824eecda getTasks" bindtap="handleProxy" data-eventid="{{'eJq-2'}}" data-comkey="{{$k}}">提交</button>
    </view>
    <view class="_view data-v-824eecda Taskhint">
      <image src="../../../static/img/Taskhint.jpg" mode="widthFix" class="_image data-v-824eecda"></image>
    </view>
  </view>
