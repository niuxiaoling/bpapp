<template name="27ace02b">
  <view class="_view data-v-61578aa0">
    <view class="_view data-v-61578aa0 item-list">
      <text class="_text data-v-61578aa0">1.提交的任务:</text>
      <text class="_text data-v-61578aa0">{{tasksDetails.taskTitle}}</text>
    </view>
    <view class="_view data-v-61578aa0 item-list">
      <text class="_text data-v-61578aa0">2.截取朋友圈图片并上传:</text>
    </view>
    <view class="_view data-v-61578aa0 item-list">
      <view class="_view data-v-61578aa0 btn-upload" bindtap="handleProxy" data-eventid="{{'sUB-0'}}" data-comkey="{{$k}}">请选择图片</view>
    </view>
    <view class="_view data-v-61578aa0 imgbox">
      <image src="{{uploadimg}}" mode="aspectFit" class="_image data-v-61578aa0 imgbox-img" bindtap="handleProxy" bindlongpress="handleProxy" data-eventid="{{'TAB-1'}}" data-comkey="{{$k}}"></image>
      <view class="_view data-v-61578aa0 progress-line">
        <progress percent="{{percent}}" stroke-width="5" class="_progress data-v-61578aa0" />
      </view>
    </view>
    <view wx:if="{{uploadimg !=''}}" class="_view data-v-61578aa0">
      <button class="_button data-v-61578aa0 getTasks" bindtap="handleProxy" data-eventid="{{'z4U-2'}}" data-comkey="{{$k}}">提交</button>
    </view>
    <view class="_view data-v-61578aa0 Taskhint">
      <image src="../../../static/img/Taskhint.jpg" mode="widthFix" class="_image data-v-61578aa0"></image>
    </view>
  </view>
</template>