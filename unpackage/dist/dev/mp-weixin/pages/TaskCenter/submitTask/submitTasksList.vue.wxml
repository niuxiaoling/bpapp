<template name="2c4834fe">
  <view class="_view data-v-b41bd39a hall">
    <block wx:if="{{submitList.length >0}}">
      <view class="_view data-v-b41bd39a task" wx:key="tasks" key="{{tasks}}" bindtap="handleProxy" data-eventid="{{'xcK-0-'+index}}" data-comkey="{{$k}}" wx:for="{{submitList}}" wx:for-index="index" wx:for-item="tasks">
        <view class="_view data-v-b41bd39a subcontent">
          <image src="../../../static/img/logoicon1.png" mode="aspectFit" class="_image data-v-b41bd39a"></image>
          <text class="_text data-v-b41bd39a">{{tasks.taskInfo.taskTitle}}</text>
          <text style="color: #999999;" class="_text data-v-b41bd39a">（{{tasks.taskInfo.taskType == '01'?'勇士':'自由'}}）</text>
        </view>
        <view class="_view data-v-b41bd39a">
          <text style="color: #666666;" class="_text data-v-b41bd39a">{{tasks.orderStatus == '1'?'待提交':'已提交'}}</text>
          <image class="_image data-v-b41bd39a you" src="../../../static/img/youjiantou.png" mode="aspectFit"></image>
        </view>
      </view>
    </block>
    <view wx:else class="_view data-v-b41bd39a"> 暂无数据 </view>
  </view>
</template>