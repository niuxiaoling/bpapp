
.logo image {
  width: 100%;
}
.bevip {
  margin: 40rpx auto 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
  justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.bevip .bevip-item {
  padding: 0 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 40rpx;
  border: 1px solid #444;
  border-radius: 5px;
}
.bevip .bevip-item.active {
  border: 1px solid red;
}
.bevipbtn {
  margin: 60rpx auto 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.bevipbtn ._button {
  background: red;
  color: #fff;
  border-radius: 5px;
}