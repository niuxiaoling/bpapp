
.logo image {
  width: 100%;
}
.bevip {
  margin: 40rpx auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
  justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.bevipbtn {
  margin: 60rpx auto 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.bevipbtn view {
  margin-bottom: 50rpx;
  width: 50%;
  height: 75rpx;
  background: none;
}
.bevipbtn view image {
  width: 100%;
  height: 75rpx;
}
.textcontents {
  width: 85%;
  margin: 0 auto;
}
.wptext {
  font-size: 25rpx;
  color: #333333;
}
.wptextgr {
  font-size: 25rpx;
  color: #999999;
