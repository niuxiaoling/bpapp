
.swiperbox {
  width: 100%;
  background: #fff;
}
.banner {
  width: 100%;
  height: 100rpx;
  background: #EDEDED;
  line-height: 100rpx;
}
.marquee_box {
  position: relative;
}
.list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  background: #fff;
}
.list .list_item {
  width: 33.3%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding-top: 12rpx;
}
.list .list_item navigator {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.list_item image {
  width: 80rpx;
  height: 80rpx;
}
.list_item text {
  margin-top: 18rpx;
}